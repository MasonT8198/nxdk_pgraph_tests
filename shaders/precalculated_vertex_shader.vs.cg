struct vIn {
  float3 norm : NORMAL;
  float2 tex : TEXCOORD;
  float3 pos : POSITION;
  float4 diffuse : DIFFUSE;
};

struct vOut {
  float4 pos : POSITION;
  float4 col : COLOR;
  float2 tex0 : TEXCOORD0;  // Our diffuse map
};

vOut main(vIn I) {
  vOut result;

  result.pos = float4(I.pos.xyz, 1.0);
  result.col = float4(1.0);
  result.tex0 = I.tex;

  return result;
}
